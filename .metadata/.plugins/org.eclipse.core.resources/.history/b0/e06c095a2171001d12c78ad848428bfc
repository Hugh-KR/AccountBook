<mapper namespace="mapper.chartMapper">
<select id = "getWomenCount" resultType="int">
select count(*) from almom.user where user_sex = 0;

</select>
<select id = "getMenCount" resultType="int">
select count(*) from almom.user where user_sex = 1;
</select>
</mapper>