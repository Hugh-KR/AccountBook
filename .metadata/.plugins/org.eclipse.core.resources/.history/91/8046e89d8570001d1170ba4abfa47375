<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="account">
<select id ="deposit" parameterType="accountVO" resultType="accountVO">
	select * from accountbook where paydate = #{paydate} and payselect = '수입'
</select> <!-- 입금 -->


<select id="withdraw" parameterType="accountVO" resultType="accountVO">
	select * from accountbook where paydate = #{paydate} and payselect = '지출'

</select> <!-- 출금 -->
</mapper>