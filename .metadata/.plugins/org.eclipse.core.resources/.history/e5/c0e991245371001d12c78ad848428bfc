<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.accountMapper">
	<select id="countTurtle" resultType="com.multi.mvc.AccountVO">
		select * from AccountBook
		where mb_id=#{mb_id} and pos_type=#{pos_type}
		and pos_time between DATE_SUB(NOW(), INTERVAL 7 DAY) and NOW()
		order by paydate;
	</select>
</mapper>